{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.3|^8.0",
        "anlutro/l4-settings": "^1.0",
        "barryvdh/laravel-translation-manager": "^0.5.10",
        "hesto/multi-auth": "^2.0",
        "illuminate/support": "^8.38",
        "laravel/framework": "^8.0",
        "laravel/passport": "^10.1",
        "laravel/socialite": "^5.2",
        "laravel/ui": "^3.2",
        "mjaschen/phpgeo": "^3.2",
        "nesbot/carbon": "^2.46",
        "stripe/stripe-php": "^7.77"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3",
        "symfony/css-selector": "5.2.4",
        "symfony/dom-crawler": "5.2.4"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helper/ControllerHelper.php",
            "app/Helper/ViewHelper.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
